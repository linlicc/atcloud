package br.edu.infnet.modelo;

import br.edu.infnet.auxiliar.TipoHamburguer;

public class Hamburguer extends Lanche {
	
	private TipoHamburguer tipo;
	
	public Hamburguer(TipoHamburguer tipo, Integer quantidade) {
		super(TipoHamburguer tipo, Integer quantidade);		
		this.tipo = tipo;
	}

	public TipoHamburguer getTipo() {
		return tipo;
	}

	public void setTipo(TipoHamburguer tipo) {
		this.tipo = tipo;
	}

	@Override
	public String toString() {
		return "Hamburguer [tipo=" + tipo + "]";
	}

}
